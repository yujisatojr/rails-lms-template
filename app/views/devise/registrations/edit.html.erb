<% title t('devise.registrations.edit.edit_profile_title') %>
<div class="container-parent-wrapper">
  <h3><%= t('devise.registrations.edit.set_profile_picture_header') %></h3>
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put }) do |f| %>
    <%= render "devise/shared/error_messages", resource: resource %>
    <br>
    <br>
    <br>
    <%= f.submit t('devise.registrations.edit.save_changes_button'), class: "btn btn-outline-dark" %>
  <% end %>
</div>